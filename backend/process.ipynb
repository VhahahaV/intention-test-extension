   "source": [
    "import json\n",
    "\n",
    "fp1 = './Prompt-Ours.json'\n",
    "fp2 = './log.txt'\n",
    "with open(fp1, 'r') as f:\n",
    "    data = json.load(f)\n",
    "\n",
    "data = data[0]\n",
    "with open(fp2, 'w', encoding = 'utf8') as f:\n",
    "    for m in data['messages']:\n",
    "        f.write(f\"[{m['role']}]\\n\")\n",
    "        f.write(m['content'] + '\\n') "
   ]
  }
